 filters: {\r\n    subcontent: (content) => {\r\n      return content.substring(0, 80) + \"...\";\r\n    },\r\n    toDate: (timestamp) => {\r\n      const data = new Date(timestamp);\r\n      let Y = data.getFullYear();\r\n      let M = data.getMonth();\r\n      let D = data.getDate();\r\n      let H = data.getHours();\r\n      let min = (Array(2).join(\"0\") + data.getMinutes()).slice(-2);\r\n      return `${Y}/${M + 1}/${D} ${H}:${min}`;\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Admin"}]}